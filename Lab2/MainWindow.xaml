<Window x:Class="Lab2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="selectApp" HorizontalAlignment="Left" Margin="340,130,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="selectApp_SelectionChanged">

            <ComboBoxItem Content="Population"/>
            <ComboBoxItem Content="Shipping chrages"/>
            <ComboBoxItem Content="DistanceTraveled"/>
            <ComboBoxItem Content="TestScores"/>

        </ComboBox>
        <Button Content="Open " HorizontalAlignment="Left" Margin="363,248,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label x:Name="Title" Content="Select the Application " HorizontalAlignment="Center" Margin="0,63,0,0" VerticalAlignment="Top" Width="222" RenderTransformOrigin="0.179,1.059" FontSize="20" FontWeight="Bold"/>

    </Grid>
</Window>
